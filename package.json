{
  "name": "mido-rc",
  "version": "1.0.0",
  "description": "react组件库",
  "author": "Midnight Blues",
  "bugs": {},
  "main": "lib/main.js",
  "license": "ISC",
  "scripts": {
    "start": "npm run dev",
    "creat": "node ./config/getProject.js",
    "cp": "node ./config/creatProject.js",
    "dev": "rm -rf ./source && npm run creat && mido dev -p 9001",
    "build": "rm -rf ./source && cross-env NODE_ENV=production npm run creat && cross-env NODE_ENV=production mido build",
    "lib": "rm -rf ./lib && babel src --out-dir lib --configFile ./config/babel.config.js",
    "publish": "npm run lib && npm publish",
    "lint": "esw ./config/*.* ./src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --fix --watch",
    "lint:style": "stylelint --fix --syntax less src/web/**/*.less",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "art-template": "^4.13.2",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.12.0",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-mido": "^1.0.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-watch": "^5.1.2",
    "fs-extra": "^8.0.1",
    "glob": "^7.1.4",
    "lint-staged": "^9.2.0",
    "mido-construct": "^1.2.0",
    "pre-commit": "^1.2.2",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0"
  },
  "dependencies": {
    "antd": "^3.20.2",
    "antd-mobile": "^2.2.14"
  },
  "lint-staged": {
    "{src,config}/**/*.{js,jsx,vue}": [
      "esw --fix",
      "git add"
    ],
    "*.md": [
      "esw --ext '.md' --fix",
      "git add"
    ],
    "*.less": [
      "stylelint --fix --syntax less",
      "git add"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ]
}
