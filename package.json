{
  "name": "mido-rc",
  "version": "1.0.0",
  "description": "react组件库",
  "author": "Midnight Blues",
  "bugs": {},
  "main": "lib/main.js",
  "license": "ISC",
  "scripts": {
    "start": "rm -rf ./source && npm run creat && npm run dev",
    "creat": "node ./config/getProject.js",
    "cp": "node ./config/creatProject.js",
    "dev": "mido dev -p 9001",
    "build": "cross-env NODE_ENV=production mido build",
    "lib": "rm -rf ./lib && babel src --out-dir lib --copy-files --ignore **.test.js,**.md,examples/**",
    "publish": "npm run lib && npm publish",
    "lint": "esw ./config/*.* ./src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --fix --watch",
    "lint:style": "stylelint --fix --syntax less src/**/*.less"
  },
  "devDependencies": {
    "art-template": "^4.13.2",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-mido": "^1.0.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-watch": "^5.1.2",
    "fs-extra": "^8.0.1",
    "glob": "^7.1.4",
    "mido-construct": "^1.2.0",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0"
  }
}
